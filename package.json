    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "axios": "^0.26.0",
    "cross-fetch": "^3.1.5",
    "date-fns": "^2.28.0",
    "graphql": "^16.3.0",
    "react-plaid-link": "^3.3.0"
  }
