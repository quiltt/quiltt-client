{
  "name": "@quiltt/client",
  "version": "0.1.2",
  "description": "Quiltt client to use the Quiltt API with React",
  "author": "Zubair Aziz",
  "license": "MIT",
  "repository": "quiltt/quiltt-client",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "files": [
    "dist"
  ],
  "types": "dist/src/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "test": "jest",
    "coverage": "jest --collect-coverage",
    "clean": "rimraf dist",
    "build": "rollup -c",
    "build:prod": "yarn clean && rollup -c",
    "prepare": "yarn build:prod",
    "develop": "rollup -c -w",
    "start": "yarn develop",
    "publish-script": "npm publish --access public",
    "lint": "eslint --ext .tsx --ext .ts src/ && eslint --ext .tsx --ext .ts example/src/",
    "lint:fix": "eslint --ext .tsx --ext .ts src/ --fix && eslint --ext .tsx --ext .ts example/src/ --fix",
    "prettier": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,css,md,json}\" \"./example/src/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "format": "yarn lint:fix && yarn prettier",
    "type-check": "tsc"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@apollo/client": "^3.5.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
