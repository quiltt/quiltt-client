    "factory.ts": "^0.5.2",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-watch-typeahead": "^1.0.0",
    "lint-staged": "^12.3.4",
    "np": "^7.6.0",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.68.0",
    "rollup-plugin-babel": "^4.4.0",
